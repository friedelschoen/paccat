output {
    name = "textselect",

    fetch = import ./name.pcr (name="hello", world="1"),

    repo = output {
        name = "fetch-textselect",
        build = "git clone https://github.com/friedelschoen/textselect.git ."
    },

    script = ''
        make
        mkdir -p {{ $out }}/bin {{ $out }}/share/man/man1
        make PREFIX={{ $out }} install
    ''
}
"ello"